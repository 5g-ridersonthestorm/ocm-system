          "$id": "#/properties/UsageType/properties/ID",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            4
          ]
        },
        "Title": {
          "$id": "#/properties/UsageType/properties/Title",
          "type": "string",
